function syncMarker(latLng) {
  console.log($('#map-canvas').data())
  $.ajax({
    method: "POST",
    url: "http://localhost:3000/waypoints",
    data: {latitude: latLng.D, longitude: latLng.k, journey: $('#map-canvas').data("journeyid")},
    success: function(res) {
      $('.waypoint-list').append(res)
    }
  })
}